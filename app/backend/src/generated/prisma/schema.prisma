datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

// Prisma schema for mood input
// Add this model to enable storing user mood data
model Mood {
  id        Int      @id @default(autoincrement())
  userId    String // Optionally associate with a user
  mood      String
  createdAt DateTime @default(now())
}

model Sample {
  id        Int      @id @default(autoincrement())
  data      String
  createdAt DateTime @default(now())
}
